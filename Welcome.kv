#: import Welcome Welcome

<WelcomeScreen>:
	BoxLayout:
		orientation: "vertical"

		canvas:
			Color:
				rgba: 1,0,0,1
			Rectangle:
				size: self.width,self.height
			Color:
				rgba: 1,1,1,1
			Ellipse:
				size:self.width,350
				pos:70,150
			Ellipse:
				size:100,100
				pos:-20,-50
			Color:
				rgba: 1,1,1,1
			Ellipse:
				size:400,400
				pos:-200,450
			Color:
				rgba: 1,0,0,1
			Ellipse:
				size:250,250
				pos:-100,525

		Label:

			canvas:
				Color:
					rgba: 1,1,1,1
				Rectangle:
					size:240,240
					pos: 110,230
					source: "Layout/Logo runistica.png"

		Button:
			id: _EnterButton
			text: "Enter"
			color: 1,1,1,1
			background_color:0,0,1,0
			size_hint: (.2,.1)
			pos_hint:{"center_x":0.75, "center_y":0}
			on_press: root.manager.current = "settingsscreen"
